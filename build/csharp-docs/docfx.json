{
  "metadata": [
    {
      "src": [
        {
          "src": "../../src",
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "JsonSchema/**",
            "Umbraco.Cms.StaticAssets/**",
            "Umbraco.Web/**",
            "Umbraco.Web.UI/**",
            "Umbraco.Web.UI.Client/**",
            "Umbraco.Web.UI.Docs/**"
          ]
        }
      ],
      "dest": "api",
      "filter": "docfx.filter.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "toc.yml",
          "index.md"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Umbraco C# API Docs",
      "_enableSearch": true,
      "_disableContribution": true
    },
    "dest": "_site",
    "template": [
      "default",
      "umbracotemplate"
    ]
  }
}
