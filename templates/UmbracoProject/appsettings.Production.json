{
  "$schema": "./appsettings-schema.json",
  "Umbraco": {
    "CMS": {
      "Runtime": {
        "Mode": "Production"
      },
      "Global": {
        "UseHttps": true
      },
      "ModelsBuilder": {
        "ModelsMode": "Nothing"
      },
      "WebRouting": {
        "UmbracoApplicationUrl": "https://localhost:HTTPS_PORT_FROM_TEMPLATE"
      }
    }
  }
}
